{
  "manifest_version": 3,
  "name": "EveryDollar Extensions",
  "version": "0.3",
  "icons": {
    "32": "everydollar-32x32.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "run_at": "document_idle",
      "matches": [
        "https://www.everydollar.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
